      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

//fetching currentdate

var today = new Date();
var dd = today.getDate();
var mm = today.getMonth()+1; 
var yyyy = today.getFullYear();
if(dd&lt;10) 
{
    dd='0'+dd;
} 

if(mm&lt;10) 
{
    mm='0'+mm;
} 
today = yyyy+'/'+mm+'/'+dd;

//formating dob,anniverasary dates and spliiting dates into 3 parts

var dobformat = date2str(dob.getDate(),"yyyy/MM/dd");
var workavrsydateformat = date2str(workavrsydate.getDate(),"yyyy/MM/dd");
var dobparts1=dobformat.toString().split("/");
var currentparts1=today.toString().split("/");
var workavrsydateparts1=workavrsydateformat.toString().split("/");
var dobmonth=dobparts1[1];
